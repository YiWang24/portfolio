# RAG Sync Configuration
# These environment variables are required for syncing documentation 
# to the backend vector database during build time.

# Backend API URL (defaults to https://api.yiw.me)
BACKEND_URL=https://api.yiw.me

# RAG Sync API Key - must match backend RAG_SYNC_KEY
RAG_SYNC_KEY=your_rag_sync_key_here

# Cloudflare Access credentials (for CF protected backends)
CF_CLIENT_ID=your_cf_client_id
CF_CLIENT_SECRET=your_cf_client_secret

# Set to 'true' to fail the build if RAG sync fails (default: false)
RAG_SYNC_REQUIRED=false
